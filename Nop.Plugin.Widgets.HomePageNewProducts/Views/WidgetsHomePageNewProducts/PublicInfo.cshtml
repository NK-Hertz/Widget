@model IList<ProductOverviewModel>

@using Nop.Web.Models.Catalog;
@*@using Nop.Core.Domain.Catalog;*@
@if (Model.Count > 0)
{
    <div class="product-grid home-page-new-product-grid">
        <div class="title">
            <strong>
                @*@T("HomePage.Products")*@
                Newest Products
            </strong>
        </div>
        <div class="item-grid">
            @foreach (var item in Model)
            {
                <div class="item-box">
                    @Html.Partial("_ProductBox", item)
                </div>
            }
        </div>
    </div>
}